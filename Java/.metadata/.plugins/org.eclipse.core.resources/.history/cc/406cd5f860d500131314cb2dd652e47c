import org.jfugue.*; 

public class MyMusicApp {

	public static void main(String[] args) {
		Player player = new Player();
		Pattern pattern1 = new Pattern("T140 X[Volume]=1 I[Piano] V0 A B C");
		Pattern pattern2 = new Pattern("T140 X[Volume]=1000 I[Flute] V1 D E F");
		Pattern song = new Pattern();
		song.add(pattern1);
		song.add(pattern2);
		player.play(song);
		System.exit(0);
		// If using Java 1.4 or lower
	}

}
