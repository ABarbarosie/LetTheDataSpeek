import org.jfugue.*; 

public class MyMusicApp {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		Player player = new Player();
		Pattern pattern1 = new Pattern("T126 I[Piano] V0 A1 B1 C1");
		Pattern pattern2 = new Pattern("T126 I[Metallic] V1 D3 E3 F3");
		Pattern song = new Pattern();
		song.add(pattern1);
		song.add(pattern2);
		player.play(song);
		System.exit(0);
		// If using Java 1.4 or lower
	}

}
